# Finish the pow_two() function
pow_two <- function(x, print_info = TRUE) {
  y <- x ^ 2
  
  if(print_info == TRUE){
    print(paste(x, "to the power two equals", y))
  }
  
  return(y)
}

# Output
# if pow_two(X) with no print_info specified, then default = TRUE #
# > pow_two(5)
# [1] "5 to the power two equals 25"
# [1] 25
# > pow_two(5, TRUE)
# [1] "5 to the power two equals 25"
# [1] 25
# > pow_two(5, FALSE)
# [1] 25

